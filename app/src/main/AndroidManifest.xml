<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.comtracksapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Email"></activity>
        <activity android:name=".Text" />
        <activity android:name=".Call" />
        <activity android:name=".DeleteContact" />
        <activity android:name=".edit_contact" />
        <activity android:name=".activity_contact_profile" />
        <activity android:name=".AddNewContact" />
        <activity
            android:name=".ContactProfile"
            android:label="@string/title_activity_contact_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Home" />
        <activity android:name=".AllContacts" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>